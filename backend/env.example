# Configurações do Servidor
NODE_ENV=development
PORT=4000
LOG_LEVEL=info

# Banco de Dados
MONGO_URI=mongodb://localhost:27017/moonlightlogger
REDIS_URL=redis://localhost:6379

# Configurações de IA - Pipeline Híbrido
AI_PROVIDER=offline
# Opções: offline (apenas heurísticas), hybrid (heurísticas + IA), ai-only (apenas IA)
AI_ENABLED=false
# OpenAI Configuration (opcional - só usado se AI_PROVIDER != offline)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_BASE_URL=https://api.openai.com/v1

# Configurações de Segurança
JWT_SECRET=your_jwt_secret_here
CORS_ORIGIN=http://localhost:3000

# Configurações de Log
LOG_RETENTION_DAYS=30
LOG_BATCH_SIZE=100
