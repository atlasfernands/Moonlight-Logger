{
  "port": 3001,
  "frontendPort": 3000,
  "logLevel": "info",
  "enableRealTime": true,
  "autoStart": true,
  "features": {
    "errorTracking": true,
    "performanceMonitoring": true,
    "realTimeUpdates": true,
    "aiAnalysis": true,
    "offlineMode": true,
    "intelligentAlerts": true,
    "logClustering": true,
    "trendAnalysis": true,
    "slackDiscordIntegration": true,
    "sentimentAnalysis": true
  },
  "scaling": {
    "threshold": 1000,
    "autoScale": true,
    "redisEnabled": false
  },
  "ui": {
    "theme": "dark",
    "refreshInterval": 5000,
    "maxLogsDisplay": 1000
  },
  "alerts": {
    "enabled": true,
    "thresholds": {
      "errorSpike": 5,
      "warningFlood": 20,
      "criticalError": 1,
      "performanceDrop": 0.8,
      "memoryLeak": 0.9
    },
    "notifications": {
      "email": false,
      "slack": false,
      "discord": false,
      "teams": false
    }
  },
  "clustering": {
    "enabled": true,
    "similarityThreshold": 0.7,
    "maxClusterSize": 100,
    "autoCleanup": true,
    "cleanupInterval": 86400000
  },
  "trends": {
    "enabled": true,
    "timeWindows": ["hour", "day", "week", "month"],
    "anomalyDetection": true,
    "forecasting": true,
    "seasonalityAnalysis": true
  },
  "integrations": {
    "slack": {
      "enabled": false,
      "webhookUrl": "",
      "channels": ["#alerts", "#errors"],
      "notificationLevels": ["error", "warning", "alert"]
    },
    "discord": {
      "enabled": false,
      "webhookUrl": "",
      "channels": ["#alerts", "#errors"],
      "notificationLevels": ["error", "warning", "alert"]
    },
    "teams": {
      "enabled": false,
      "webhookUrl": "",
      "channels": ["alerts", "errors"],
      "notificationLevels": ["error", "warning", "alert"]
    },
    "email": {
      "enabled": false,
      "smtp": {
        "host": "",
        "port": 587,
        "secure": false,
        "user": "",
        "pass": ""
      },
      "recipients": [],
      "notificationLevels": ["error", "alert"]
    }
  },
  "sentiment": {
    "enabled": true,
    "analysisDepth": "full",
    "emotionDetection": true,
    "contextWeighting": true,
    "confidenceThreshold": 0.6,
    "batchAnalysis": true
  },
  "ai": {
    "enabled": true,
    "providers": ["local", "openai", "anthropic"],
    "localModel": "sentiment-analyzer",
    "openai": {
      "apiKey": "",
      "model": "gpt-3.5-turbo",
      "maxTokens": 1000
    },
    "anthropic": {
      "apiKey": "",
      "model": "claude-3-sonnet",
      "maxTokens": 1000
    }
  },
  "performance": {
    "monitoring": true,
    "metrics": ["cpu", "memory", "disk", "network"],
    "samplingInterval": 5000,
    "retentionPeriod": 604800000
  },
  "security": {
    "authentication": false,
    "rateLimiting": true,
    "maxRequestsPerMinute": 1000,
    "allowedOrigins": ["http://localhost:3000"],
    "apiKeys": []
  },
  "export": {
    "enabled": true,
    "formats": ["json", "csv", "pdf"],
    "scheduledExports": false,
    "retentionPeriod": 2592000000
  }
}
